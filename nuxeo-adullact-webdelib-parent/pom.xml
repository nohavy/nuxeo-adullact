<?xml version="1.0" encoding="UTF-8"?>
<project>
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.nuxeo.ecm.platform</groupId>
    <artifactId>nuxeo-features-parent</artifactId>
    <version>7.4</version>
  </parent>
  <groupId>org.nuxeo.adullact.webdelib</groupId>
  <artifactId>nuxeo-adullact-webdelib-parent</artifactId>
  <version>7.4</version>
  <name>Parent Project for adullact webdelib integration</name>
  <packaging>pom</packaging>
  <description/>
  <modules>
    <module>nuxeo-adullact-webdelib-core</module>
    <module>nuxeo-adullact-webdelib-mp</module>
  </modules>
  <properties>
    <nuxeo.adullact.webdelib.version>7.4</nuxeo.adullact.webdelib.version>
    <nuxeo.adullact.webdelib.studio.version>1.0.5</nuxeo.adullact.webdelib.studio.version>
    <nuxeo.xml.parser.version>7.4</nuxeo.xml.parser.version>
  </properties>
  <dependencyManagement>
    <dependencies>
    <dependency>
      <groupId>org.nuxeo.ecm.platform</groupId>
      <artifactId>nuxeo-importer-xml-parser</artifactId>
      <version>${nuxeo.xml.parser.version}</version>
    </dependency>
    <dependency>
      <groupId>org.nuxeo.adullact.webdelib</groupId>
      <artifactId>adullact-webdelib</artifactId>
      <version>${nuxeo.adullact.webdelib.studio.version}</version>
    </dependency>
    <dependency>
      <groupId>org.nuxeo.adullact.webdelib</groupId>
      <artifactId>nuxeo-adullact-webdelib-mp</artifactId>
      <version>${nuxeo.adullact.webdelib.version}</version>
    </dependency>
    <dependency>
      <groupId>org.nuxeo.adullact.webdelib</groupId>
      <artifactId>nuxeo-adullact-webdelib-core</artifactId>
      <version>${nuxeo.adullact.webdelib.version}</version>
    </dependency>
   </dependencies>
  </dependencyManagement>
  <repositories>
    <repository>
      <id>public</id>
      <url>http://maven.nuxeo.org/public</url>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
    <repository>
      <id>public-snapshot</id>
      <url>http://maven.nuxeo.org/public-snapshot</url>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots>
        <updatePolicy>always</updatePolicy>
        <enabled>true</enabled>
      </snapshots>
    </repository>

    <repository>
      <id>nuxeo-studio</id>
      <url>https://connect.nuxeo.com/nuxeo/site/studio/maven</url>
      <releases>
          <enabled>true</enabled>
      </releases>
      <snapshots>
          <enabled>true</enabled>
      </snapshots>
    </repository>
  </repositories>
</project>
